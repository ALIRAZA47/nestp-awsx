{
  "name": "@nestp/awsx",
  "version": "0.1.0",
  "description": "One-stop AWS integrations for NestJS with unified config and CLI",
  "license": "MIT",
  "author": "ALIRAZA47",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ALIRAZA47/nestp-awsx.git"
  },
  "bugs": {
    "url": "https://github.com/ALIRAZA47/nestp-awsx/issues"
  },
  "homepage": "https://github.com/ALIRAZA47/nestp-awsx#readme",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "require": "./dist/cli/index.cjs"
    }
  },
  "bin": {
    "awsx": "dist/cli/index.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "npm run build",
    "docs:build": "cd docs && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "nestjs",
    "aws",
    "s3",
    "sqs",
    "ses",
    "route53",
    "sdk",
    "cloud"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-route-53": "^3.552.0",
    "@aws-sdk/client-s3": "^3.552.0",
    "@aws-sdk/client-ses": "^3.552.0",
    "@aws-sdk/client-sqs": "^3.552.0",
    "@aws-sdk/credential-provider-node": "^3.552.0",
    "@aws-sdk/lib-storage": "^3.552.0",
    "@aws-sdk/s3-request-presigner": "^3.552.0",
    "commander": "^11.1.0",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/prompts": "^2.4.7",
    "@types/node": "^20.11.30",
    "reflect-metadata": "^0.1.13",
    "tsdown": "^0.20.1",
    "typescript": "^5.4.2",
    "vitest": "^2.1.0"
  }
}
